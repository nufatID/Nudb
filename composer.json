{
  "name": "nufat/nudb",
  "description": "A pure PHP WebSocket client for communicating with Nudb",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nunu Fatulah",
      "email": "nunu.fatulah@nufat.id"
    }
  ],
  "require": {
    "php": ">=7.4",
    "textalk/websocket": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  },
  "autoload": {
    "psr-4": {
      "Nufat\\Nudb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}